{
  "version": 2,
  "scope": "telemark",
  "name": "ms-shared-api-faktura",
  "alias": [
    "faktura.api.service.t-fk.no"
  ],
  "env": {
    "NODE_ENV": "production",
    "MONGODB_CONNECTION": "@db-cosmostest-felles",
    "MONGODB_COLLECTION": "fakturagrunnlag",
    "MOA_TENANT_ID": "@moa_tenant_id",
    "JWT_SECRET": "@tfk-jwt-secret",
    "PAPERTRAIL_HOST": "@tfk-papertrail-host",
    "PAPERTRAIL_PORT": "@tfk-papertrail-port",
    "PAPERTRAIL_HOSTNAME": "elevpc"
  },
  "builds": [
    { "src": "/index.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
